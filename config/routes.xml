<?xml version="1.0" encoding="utf-8" ?>
<routes>
	<route url="/" app="Frontend" module="Main" action="index" ></route>
	<!-- NEWS -->
	<route url="/news-([0-9]+)\.html" app="Frontend" module="News" action="show" vars="id"></route>
	<route url="/commenter-([0-9]+)\.html" app="Frontend" module="News" action="insertComment" vars="news" ></route>
	<!-- CONNEXION -->
	<route url="/connexion-espace.html" app="Frontend" module="Connexion" action="index" ></route>
	<route url="/deconnexion-espace.html" app="Frontend" module="Connexion" action="deco" ></route>
    <!-- RESULTATS -->
    <route url="/saisie.html" app="Frontend" module="Saisie" action="index" ></route>
	<route url="/presence-([0-9]+)\.html" app="Frontend" module="Saisie" action="presence" vars="id_dossier"></route>
	<route url="/dossier-([0-9]+)\.html" app="Frontend" module="Saisie" action="dossier" vars="id_dossier"></route>
    <route url="/resultats-([0-9]+)\.html" app="Frontend" module="Saisie" action="resultats" vars="id_candidat"></route>
    <route url="/resultat-([0-9]+)\-([0-9]+)\.html" app="Frontend" module="Saisie" action="resultat" vars="id_candidat,id_category"></route>
    <!-- ADMINISTRATION -->
    <route url="/admin" app="Backend" module="Main" action="index" ></route>
	<route url="/admin/news-insert\.html" app="Backend" module="News" action="insert" ></route>
	<route url="/admin/news-update-([0-9]+)\.html" app="Backend" module="News" action="update" vars="id" ></route>
	<route url="/admin/news-delete-([0-9]+)\.html" app="Backend" module="News" action="delete" vars="id" ></route>
</routes>